  <Name value="Transaction" typeid="1" />
  <Shortcut typeid="1" value="trans" />
  <Description typeid="1" value="Template code to use a transaction in a script" />
  <_Script typeid="1" value="BEGIN TRANSACTION;&#xD;&#xA;&#xD;&#xA;	BEGIN TRY&#xD;&#xA;		--Code that will be in the transaction, any error ocurred here will be catched and the whole code rolled back&#xD;&#xA;		{cursor}&#xD;&#xA;		&#xD;&#xA;	END TRY&#xD;&#xA;	BEGIN CATCH&#xD;&#xA;		--return the data of the error&#xD;&#xA;	    SELECT &#xD;&#xA;	        ERROR_NUMBER() AS ErrorNumber&#xD;&#xA;	        ,ERROR_SEVERITY() AS ErrorSeverity&#xD;&#xA;	        ,ERROR_STATE() AS ErrorState&#xD;&#xA;	        ,ERROR_PROCEDURE() AS ErrorProcedure&#xD;&#xA;	        ,ERROR_LINE() AS ErrorLine&#xD;&#xA;	        ,ERROR_MESSAGE() AS ErrorMessage;&#xD;&#xA;	        &#xD;&#xA;		IF @@TRANCOUNT &gt; 0&#xD;&#xA;	        ROLLBACK TRANSACTION;	&#xD;&#xA;	END CATCH&#xD;&#xA;	&#xD;&#xA;	IF @@TRANCOUNT &gt; 0&#xD;&#xA;	    COMMIT TRANSACTION;" />
